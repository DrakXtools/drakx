#: printer/printerdrake.pm:4343
#, c-format
msgid "Restarting printing system..."
msgstr "Trükkimissüsteemi taaskäivitamine..."

#: printer/printerdrake.pm:4374
#, c-format
msgid "high"
msgstr "kõrge"

#: printer/printerdrake.pm:4374
#, c-format
