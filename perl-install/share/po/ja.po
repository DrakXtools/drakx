#: security/l10n.pm:17
#, c-format
msgid "Allow remote root login"
msgstr "リモート root ログインを許可"

#: security/l10n.pm:18
#, c-format
msgid "Direct root login"
msgstr "直接 root でログイン"

#: security/l10n.pm:19
