msgstr "Sistinme"

#: ../../services.pm_.c:138
msgid "Remote Administration"
msgstr "Administråcion då lon"

#: ../../services.pm_.c:146
msgid "Database Server"
msgstr "Sierveu di båze di dnêyes"

#: ../../services.pm_.c:175
#, c-format
msgid "Services: %d activated for %d registered"
msgstr "Siervices: %d en alaedje po %d eredjistrés"

