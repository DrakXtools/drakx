"\\fIarg\\fP = NONE sallitaan ainoastaan/etc/issue."

#: ../../security/help.pm:1
#, c-format
msgid ""
"Arguments: (arg)\n"
"\n"
"Allow/Forbid autologin."
msgstr ""
"Perusteet: (arg)\n"
"\n"
"Salli / Kiellä: automaattinen sisäänkirjautuminen."

#: ../../security/help.pm:1
#, c-format
msgid ""
"Arguments: (arg)\n"
"\n"
" Accept/Refuse icmp echo."
msgstr ""
"Perusteet: (arg)\n"
"\n"
" Hyväksy / Hylkää icmp echo viestit."

