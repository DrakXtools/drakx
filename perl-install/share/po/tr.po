"[--add]\n"
"  --add             - \"add a network interface\" wizard\n"
"  --del             - \"delete a network interface\" wizard\n"
"  --skip-wizard     - manage connections\n"
"  --internet        - configure internet\n"
"  --wizard          - like --add"
