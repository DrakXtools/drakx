msgid "Remote Administration"
msgstr "Amministrazzjoni remota"

#: ../../services.pm_.c:141
msgid "Database Server"
msgstr "Server Database"

#: ../../services.pm_.c:170
#, c-format
msgid "Services: %d activated for %d registered"
msgstr "Servizzi: %d imtella' għal %d reġistrati"

