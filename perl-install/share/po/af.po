msgstr "'Chkconfig' volg 'msec' se reëls"

#: security/l10n.pm:24
#, c-format
msgid "Enable \"crontab\" and \"at\" for users"
msgstr "Aktiveer \"crontab en \"at\" vir gebruikers"

#: security/l10n.pm:25
#, c-format
msgid "Syslog reports to console 12"
