TYPEMAP

const char *		T_PV
const void *		T_PV
int *			T_PV
void **			T_PACKED

enum newtFlagsSense	T_IV
enum newtGridElement	T_IV
SV **			T_SV

Newt::Component		NewtComponent
Newt::Grid		NewtGrid

INPUT
NewtComponent
	$var = ($type) SvIV((SV*)SvRV($arg))
NewtGrid
	$var = ($type) SvIV((SV*)SvRV($arg))

OUTPUT
NewtComponent
	sv_setref_pv($arg, "Newt::Component", (void*) $var);
NewtGrid
	sv_setref_pv($arg, "Newt::Grid", (void*) $var);
